{% extends 'base.html.twig' %}

{% block title %}Exam{% endblock %}

{% block body %}
    <h1>Exam</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ exam.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ exam.type }}</td>
            </tr>
            <tr>
                <th>Dateexam</th>
                <td>{{ exam.dateexam ? exam.dateexam|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Diagnostic</th>
                <td>{{ exam.diagnostic }}</td>
            </tr>
            <tr>
                <th>Data</th>
                <td>{{ exam.data ? exam.data|join(', ') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_exam_index') }}">back to list</a>

    <a href="{{ path('app_exam_edit', {'id': exam.id}) }}">edit</a>

    {{ include('exam/_delete_form.html.twig') }}
{% endblock %}
