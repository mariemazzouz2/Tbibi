<body>
    <div class="container" style="display: flex; justify-content: center; align-items: center; height: 100vh; padding: 20px;">
        <div class="form-container" style="width: 600px; padding: 30px; background-color: #ffffff; border-radius: 10px; box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);">
            {{ form_start(form) }}
                  {% if form_errors(form) %}
                        <div class="alert alert-danger">
                            <ul>
                                {% for error in form_errors(form) %}
                                    <li>{{ error }}</li>
                                {% endfor %}
                            </ul>
                        </div>
                    {% endif %}




				
                <div class="form-group" style="margin-bottom: 20px;">
                    {{ form_widget(form) }}
                </div>
                <button class="btn" style="width: 100%; padding: 12px; font-size: 18px; background-color: #007BFF; color: white; border: none; border-radius: 6px; cursor: pointer; transition: background 0.3s; margin-top: 20px;">
                    {{ button_label|default('Enregistrer') }}
                </button>
            {{ form_end(form) }}
        </div>
    </div>
</body>
